public int[] make2(int[] a, int[] b) {
  int[] arr = new int[2];
  int j = 0;
  for(int i = 0 ; i < a.length ; i++){
    if(j < 2){
      arr[j] = a[i];
      j++;
    }
  }
  if(j < 2){
    for(int k = 0 ; k < b.length ; k++){
      if(j < 2){
        arr[j] = b[k];
        j++;
      }
    }
  }
  return arr;
}
